{
  "name": "e-commerce",
  "version": "1.0.0",
  "description": "Complete E-commerce site built with Node.js, React, Redux, Express, MongoDB",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "SET NODE_ENV=DEVELOPMENT& nodemon backend/server.js",
    "prod": "SET NODE_ENV=PRODUCTION& nodemon backend/server.js",
    "seeder": "node backend/utils/seeder.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false && npm install --prefix frontend && npm run build --prefix frontend"
  },
  "author": "Harikrishna B",
  "license": "ISC",
  "dependencies": {
    "@stripe/react-stripe-js": "^1.6.0",
    "@stripe/stripe-js": "^1.22.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.27.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.11",
    "nodemailer": "^6.7.0",
    "nodemon": "^2.0.15",
    "react-js-pagination": "^3.0.3",
    "react-redux": "^7.2.6",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "stripe": "^8.191.0",
    "validator": "^13.6.0"
  },
